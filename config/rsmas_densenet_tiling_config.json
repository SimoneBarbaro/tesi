{
  "dataset": "RSMAS_data.mat",
  "model": {
    "name": "densenet",
    "freeze": false,
    "pretraining": "imagenet"
  },
  "batch_sizes": [16],
  "epochs": [50],
  "metrics": ["accuracy", "f1"],
  "preprocessing": [null, "rgb_lbp", "wavelet", "hsv", "lab"],
  "augmentation": [
    {
      "name": "tiling",
      "args": {
        "tile_size": 64
      }
    },
    {
      "name": "tiling",
      "args": {
        "tile_size": 32
      }
    },
    {
      "name": "tiling",
      "args": {
        "tile_size": 16
      }
    }
  ]
}